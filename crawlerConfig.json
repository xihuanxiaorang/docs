{
  "index_name": "vitepress",
  "start_urls": [
    "https://docs.xiaorang.fun/"
  ],
  "selectors": {
    "lvl0": {
      "selector": "",
      "default_value": "Documentation"
    },
    "lvl1": ".content h1",
    "lvl2": ".content h2",
    "lvl3": ".content h3",
    "lvl4": ".content h4",
    "lvl5": ".content h5",
    "text": ".content p, .content li",
    "lang": {
      "selector": "/html/@lang",
      "type": "xpath",
      "global": true
    }
  },
  "strip_chars": " .,;:#",
  "selectors_exclude": [
    ".sr-only"
  ],
  "custom_settings": {
    "attributesForFaceting": [
      "lang",
      "tags"
    ]
  }
}